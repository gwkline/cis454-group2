\r\n  return (\r\n    <>\r\n      <section>\r\n        <h1 className=\"page-title\">Projects</h1>\r\n        <button className=\"create-proj-btn\" onClick={viewHandler}>+ Create New Project</button>\r\n        <hr></hr>\r\n        <ProjectList projects={loadedProjects} />\r\n      </section>\r\n      <div>\r\n        {modalIsOpen && (\r\n          <NewProjectForm\r\n          onAddProject={addProjectHandler}\r\n          onClose={closeModalHandler}/>\r\n        )}\r\n        {modalIsOpen && <Backdrop onClick={closeModalHandler} />}\r\n      </div>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default Projects;\r\n","/Users/gavinkline/git-dev/cis454-group2/src/pages/login.js",[],[],"/Users/gavinkline/git-dev/cis454-group2/src/pages/accounts.js",["102","103"],[],"/Users/gavinkline/git-dev/cis454-group2/src/pages/register.js",[],[],"/Users/gavinkline/git-dev/cis454-group2/src/pages/reset.js",[],[],"/Users/gavinkline/git-dev/cis454-group2/src/pages/createproject.js",["104"],[],"/Users/gavinkline/git-dev/cis454-group2/src/components/Navbar/index.js",[],[],["105","106"],"/Users/gavinkline/git-dev/cis454-group2/src/firebase.js",[],[],"/Users/gavinkline/git-dev/cis454-group2/src/components/Projects/ProjectList.js",[],[],"/Users/gavinkline/git-dev/cis454-group2/src/components/Projects/NewProjectForm.js",[],[],["107","108"],"/Users/gavinkline/git-dev/cis454-group2/src/components/Navbar/NavbarElements.js",[],[],"/Users/gavinkline/git-dev/cis454-group2/src/components/Projects/ProjectItem.js",[],[],"/Users/gavinkline/git-dev/cis454-group2/src/components/Backdrop.js",[],[],"/Users/gavinkline/git-dev/cis454-group2/src/components/Modal.js",[],[],{"ruleId":"109","severity":1,"message":"110","line":2,"column":28,"nodeType":"111","messageId":"112","endLine":2,"endColumn":33},{"ruleId":"109","severity":1,"message":"113","line":2,"column":35,"nodeType":"111","messageId":"112","endLine":2,"endColumn":38},{"ruleId":"109","severity":1,"message":"114","line":11,"column":24,"nodeType":"111","message